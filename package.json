{
  "name": "databaker",
  "version": "0.0.1",
  "description": "Data analyze and Plot on Node.js",
  "keywords": [
    "data",
    "csv",
    "json",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/databaker.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/**/*.ts",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "[^/]*\\.test.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "canvas": "^2.5.0",
    "core-js": "^3.0.0",
    "csvtojson": "^2.0.8",
    "d3-color": "^1.2.3",
    "d3-scale": "^3.0.0",
    "json5": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.1.0",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.13.4",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "eslint": "^5.14.1",
    "jest": "^24.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.2"
  }
}
